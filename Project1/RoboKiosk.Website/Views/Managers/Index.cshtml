@model IEnumerable<data_access.Entities.Manager>

@{
    ViewData["Title"] = "Index";
}

<h1>Store Managers</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table" style="color: white; ">
    <thead>
        <tr>
            <th>
                Manger ID
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ManagerPw)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ManagerId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ManagerId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ManagerId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
